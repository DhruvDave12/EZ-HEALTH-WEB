<% layout('./layout/boilerplate.ejs') %>

<% for(let u of appots) { %>
    <div class="card">
        <div class="card-body">
          <p class="lead"><%= u.patientName %></p>
          <p class="lead"><%= u.address %></p>
          <p class="lead"><%= u.extraSymps %></p>
          <p class="lead"><%= u.field %></p>
        </div>
    </div>
<% } %>  